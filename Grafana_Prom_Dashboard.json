{
  "title": "SAOP Agent • Latency • Cost • Errors",
  "uid": "saop-core",
  "tags": ["saop", "agent", "observability"],
  "timezone": "browser",
  "editable": true,
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "name": "model",
        "type": "query",
        "datasource": null,
        "refresh": 2,
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "query": "label_values(agent_tokens_total, model)",
        "label": "Model",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "INPUT_COST_PER_K",
        "type": "textbox",
        "label": "Input $ per 1k tokens",
        "query": "0.0004",
        "current": { "text": "0.0004", "value": "0.0004" }
      },
      {
        "name": "OUTPUT_COST_PER_K",
        "type": "textbox",
        "label": "Output $ per 1k tokens",
        "query": "0.0016",
        "current": { "text": "0.0016", "value": "0.0016" }
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Latency p95 (s)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(agent_request_latency_seconds_bucket[$__rate_interval])))",
          "legendFormat": "p95"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p99 (s)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(agent_request_latency_seconds_bucket[$__rate_interval])))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests / sec",
      "gridPos": { "x": 0, "y": 8, "w": 8, "h": 7 },
      "targets": [
        {
          "expr": "sum by (model) (rate(agent_request_latency_seconds_count{model=~\"$model\"}[$__rate_interval]))",
          "legendFormat": "{{model}}"
        },
        {
          "expr": "sum(rate(agent_request_latency_seconds_count[$__rate_interval]))",
          "legendFormat": "all models",
          "hide": false
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Errors / sec",
      "gridPos": { "x": 8, "y": 8, "w": 8, "h": 7 },
      "targets": [
        {
          "expr": "sum by (reason) (rate(agent_errors_total[$__rate_interval]))",
          "legendFormat": "{{reason}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error rate (%)",
      "gridPos": { "x": 16, "y": 8, "w": 8, "h": 7 },
      "targets": [
        {
          "expr": "100 * sum(rate(agent_errors_total[$__rate_interval])) / clamp_min(sum(rate(agent_request_latency_seconds_count[$__rate_interval])), 1e-9)",
          "legendFormat": "error %"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Tokens / sec (input vs output)",
      "gridPos": { "x": 0, "y": 15, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum by (model) (rate(agent_tokens_total{type=\"input\", model=~\"$model\"}[$__rate_interval]))",
          "legendFormat": "input {{model}}"
        },
        {
          "expr": "sum by (model) (rate(agent_tokens_total{type=\"output\", model=~\"$model\"}[$__rate_interval]))",
          "legendFormat": "output {{model}}"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Estimated cost / min (USD)",
      "gridPos": { "x": 12, "y": 15, "w": 12, "h": 8 },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "" } },
      "targets": [
        {
          "expr": "60 * ( sum(rate(agent_tokens_total{type=\"input\", model=~\"$model\"}[$__rate_interval])) * $INPUT_COST_PER_K / 1000 + sum(rate(agent_tokens_total{type=\"output\", model=~\"$model\"}[$__rate_interval])) * $OUTPUT_COST_PER_K / 1000 )",
          "legendFormat": "cost/min"
        }
      ]
    }
  ]
}
